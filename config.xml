<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.gamename" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fart</name>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="" />
        <icon density="mdpi" src="" />
        <icon density="hdpi" src="" />
        <icon density="xhdpi" src="" />
        <icon density="xxhdpi" src="" />
        <icon density="xxxhdpi" src="" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="" width="180" />
        <icon height="60" src="" width="60" />
        <icon height="120" src="" width="120" />
        <icon height="76" src="" width="76" />
        <icon height="152" src="" width="152" />
        <icon height="40" src="" width="40" />
        <icon height="80" src="" width="80" />
        <icon height="57" src="" width="57" />
        <icon height="114" src="" width="114" />
        <icon height="72" src="" width="72" />
        <icon height="144" src="" width="144" />
        <icon height="167" src="" width="167" />
        <icon height="29" src="" width="29" />
        <icon height="58" src="" width="58" />
        <icon height="50" src="" width="50" />
        <icon height="100" src="" width="100" />
        <icon height="167" src="" width="167" />
    </platform>
    <preference name="orientation" value="landscape" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <engine name="android" spec="~7.1.2" />
</widget>
