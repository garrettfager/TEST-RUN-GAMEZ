<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.gamename" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Fart</name>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="" />
    <icon density="mdpi" src="" />
    <icon density="hdpi" src="" />
    <icon density="xhdpi" src="" />
    <icon density="xxhdpi" src="" />
    <icon density="xxxhdpi" src="" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="orientation" value="landscape" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
